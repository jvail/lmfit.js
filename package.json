{
  "name": "lmfit.js",
  "version": "3.0.0",
  "description": "WebAssembly build of a C library for Levenberg-Marquardt minimization and least-squares fitting",
  "main": "dist/lmfit.mjs",
  "browser": "dist/lmfit.web.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dist": "npm run rebuild",
    "build": "npm run cc && npm run cc-test && npm run js",
    "rebuild": "make clean && npm run build",
    "cc": "make lmfit",
    "js": "make node web",
    "cc-test": "make lmfit-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jvail/lmfit.js.git"
  },
  "types": "index.d.ts",
  "keywords": [
    "least-squares minimization",
    "curve fitting"
  ],
  "author": "jvail",
  "contributors": [
    "zbjornson"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/jvail/lmfit.js/issues"
  },
  "homepage": "https://github.com/jvail/lmfit.js#readme"
}
